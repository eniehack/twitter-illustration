{
  "manifest_version": 3,
  "name": "twitter-illustration",
  "version": "1.0.0",
  "description": "",
  "content_scripts": [
    {
      "matches": ["https://twitter.com/*"],
      "css": ["style.css"],
      "js": ["content-script.js"]
    }
  ],
  "background": {
    "service_worker": "background.js"
  },
  "host_permissions": ["http://localhost:3030/*"]
}
